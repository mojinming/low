fahrenheit = 0
print("Fahrenheit Celsius")
while fahrenheit <= 250:
    celsius = (fahrenheit - 32) / 1.8 # 转换为摄氏度
    print("{:5d} {:6.2f}".format(fahrenheit , celsius))
    fahrenheit = fahrenheit + 25
    
    
    
# Fahrenheit Celsius
#     0 -17.78
#    25  -3.89
#    50  10.00
#    75  23.89
#   100  37.78
#   125  51.67
#   150  65.56
#   175  79.44
#   200  93.33
#   225 107.22
#   250 121.11
